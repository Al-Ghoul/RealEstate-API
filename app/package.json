{
  "name": "real-estate",
  "version": "1.0.0",
  "description": "A real state API",
  "scripts": {
    "start": "dotenv -e .env.development.local -- node dist/server.js",
    "build": "tsc",
    "dev": "dotenv -e .env.development.local --  tsx watch src/server.ts",
    "lint": "eslint .",
    "test:smoke": "dotenv -e ./.env.test -- jest --testPathPattern=smoke --detectOpenHandles",
    "test:integration": "dotenv -e ./.env.test -- jest --testPathPattern=integration --detectOpenHandles --runInBand",
    "db:push": "dotenv -e .env.development.local -- npx drizzle-kit push",
    "db:makemigrations": "dotenv -e ./.env.development.local -- npx drizzle-kit generate",
    "db:applymigrations": "dotenv -e ./.env.development.local -- npx drizzle-kit migrate",
    "db:studio": "dotenv -e ./.env.development.local -- npx drizzle-kit studio",
    "db:reset": "dotenv -e .env.development.local -- tsx ./src/db/dropDatabase.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Al-Ghoul/RealEstate-Backend.git"
  },
  "keywords": [
    "expressjs",
    "typescript",
    "backend",
    "restful",
    "api"
  ],
  "author": "Al-Ghoul",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Al-Ghoul/RealEstate-Backend/issues"
  },
  "homepage": "https://github.com/Al-Ghoul/RealEstate-Backend#readme",
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-typescript": "7.27.0",
    "@eslint/js": "9.25.1",
    "@types/bcrypt": "5.0.2",
    "@types/express": "5.0.1",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.9",
    "@types/lodash": "4.17.16",
    "@types/multer": "1.4.12",
    "@types/nodemailer": "6.4.17",
    "@types/pg": "8.11.12",
    "@types/pug": "2.0.10",
    "@types/supertest": "6.0.3",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.8",
    "babel-jest": "29.7.0",
    "drizzle-kit": "0.30.6",
    "eslint": "9.25.1",
    "jest": "29.7.0",
    "supertest": "7.1.0",
    "ts-node": "10.9.2",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0"
  },
  "dependencies": {
    "bcrypt": "5.1.1",
    "dotenv-cli": "8.0.0",
    "drizzle-orm": "0.41.0",
    "express": "5.1.0",
    "google-auth-library": "9.15.1",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "multer": "1.4.5-lts.2",
    "nodemailer": "6.10.1",
    "pg": "8.14.1",
    "pug": "3.0.3",
    "redis": "4.7.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-themes": "1.4.3",
    "swagger-ui-express": "5.0.1",
    "zod": "3.24.2"
  }
}
