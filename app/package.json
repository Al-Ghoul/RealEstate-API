{
  "name": "real-estate",
  "version": "1.0.0",
  "description": "A real state API",
  "type": "module",
  "scripts": {
    "start": "./RealEstate-API",
    "build": " bun build --compile --minify --sourcemap ./src/server.ts --format=esm --outfile RealEstate-API",
    "build:docs": "bun generateOpenaiDocs.ts",
    "dev": "bun --watch src/server.ts",
    "lint": "eslint .",
    "db:push": "dotenv -e ./.env.development.local -- drizzle-kit push",
    "db:makemigrations": "dotenv -e ./.env.development.local -- drizzle-kit generate",
    "db:applymigrations": "dotenv -e ./.env.development.local -- drizzle-kit migrate",
    "db:studio": "dotenv -e ./.env.development.local -- npx drizzle-kit studio",
    "db:reset": "bun ./src/db/dropDatabase.ts",
    "postinstall": "bun2nix -o ../bun.nix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Al-Ghoul/RealEstate-API.git"
  },
  "keywords": [
    "expressjs",
    "typescript",
    "backend",
    "restful",
    "api"
  ],
  "author": "Al-Ghoul",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Al-Ghoul/RealEstate-API/issues"
  },
  "homepage": "https://github.com/Al-Ghoul/RealEstate-API#readme",
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-typescript": "7.27.0",
    "@eslint/js": "9.25.1",
    "@types/bun": "^1.2.11",
    "@types/compression": "^1.7.5",
    "@types/express": "5.0.1",
    "@types/jsonwebtoken": "9.0.9",
    "@types/lodash-es": "4.17.12",
    "@types/multer": "1.4.12",
    "@types/nodemailer": "6.4.17",
    "@types/pg": "8.11.12",
    "@types/pug": "2.0.10",
    "@types/response-time": "^2.3.8",
    "@types/supertest": "6.0.3",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.8",
    "drizzle-kit": "0.30.6",
    "eslint": "9.25.1",
    "supertest": "7.1.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0"
  },
  "dependencies": {
    "@types/morgan": "^1.9.9",
    "blurhash": "2.0.5",
    "compression": "^1.8.0",
    "dotenv-cli": "8.0.0",
    "drizzle-orm": "0.41.0",
    "express": "5.1.0",
    "file-type": "20.5.0",
    "google-auth-library": "9.15.1",
    "jsonwebtoken": "9.0.2",
    "lodash-es": "4.17.21",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.2",
    "nodemailer": "6.10.1",
    "pg": "8.14.1",
    "pug": "3.0.3",
    "redis": "4.7.0",
    "response-time": "^2.3.3",
    "rotating-file-stream": "^3.2.6",
    "sharp": "0.34.1",
    "swagger-jsdoc": "6.2.8",
    "swagger-themes": "1.4.3",
    "swagger-ui-express": "5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "3.24.2"
  }
}
