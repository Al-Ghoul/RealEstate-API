{
  "name": "real-estate",
  "version": "1.0.0",
  "description": "A real state API",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "dotenv -e .env.development.local --  tsx watch src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:push": "dotenv -e .env.development.local -- npx drizzle-kit push",
    "db:makemigrations": "dotenv -e ./.env.development.local -- npx drizzle-kit generate",
    "db:runmigrations": "dotenv -e ./.env.development.local -- npx drizzle-kit migrate",
    "db:studio": "dotenv -e ./.env.development.local -- npx drizzle-kit studio",
    "db:reset": "dotenv -e .env.development.local -- tsx ./src/db/dropDatabase.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Al-Ghoul/RealEstate-App.git"
  },
  "keywords": [
    "expressjs",
    "typescript",
    "restful",
    "API"
  ],
  "author": "Al-Ghoul",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Al-Ghoul/RealEstate-App/issues"
  },
  "homepage": "https://github.com/Al-Ghoul/RealEstate-App#readme",
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/express": "5.0.1",
    "@types/jsonwebtoken": "9.0.9",
    "@types/nodemailer": "6.4.17",
    "@types/pg": "8.11.12",
    "@types/pug": "2.0.10",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.8",
    "drizzle-kit": "0.30.6",
    "tsx": "4.19.3",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "bcrypt": "5.1.1",
    "dotenv-cli": "8.0.0",
    "drizzle-orm": "0.41.0",
    "express": "5.1.0",
    "jsonwebtoken": "9.0.2",
    "nodemailer": "6.10.1",
    "pg": "8.14.1",
    "pug": "3.0.3",
    "redis": "4.7.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-themes": "1.4.3",
    "swagger-ui-express": "5.0.1",
    "zod": "3.24.2"
  }
}
